{
  "name": "form-interactions",
  "version": "1.0.0",
  "description": "Shows how to change the value of a range input",
  "scripts": {
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "cypress:run:firefox": "npx cypress run --browser firefox",
    "cypress:run:record": "npx cypress run --record",
    "dev": "npx start-test 7072 cypress:open",
    "start": "node server.js --port 7072",
    "test:ci": "npx start-test 7072 cypress:run",
    "test:ci:firefox": "npx start-test 7072 cypress:run:firefox",
    "test:ci:record": "npx start-test 7072 cypress:run:record"
  },
  "devDependencies": {
    "cypress": "^13.3.2",
    "start-server-and-test": "^2.0.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "minimist": "^1.2.8",
    "path": "^0.12.7"
  }
}
